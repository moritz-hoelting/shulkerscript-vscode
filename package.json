{
  "name": "shulkerscript-lang",
  "displayName": "Shulkerscript Syntax Highlighting",
  "description": "Provide syntax highlighting for the Shulkerscript language",
  "version": "0.0.1",
  "publisher": "moritz-hoelting",
  "author": {
    "name": "Moritz HÃ¶lting",
    "email": "moritz@hoelting.dev",
    "url": "https://hoelting.dev"
  },
  "license": "MIT",
  "homepage": "https://shulkerscript.hoelting.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/moritz-hoelting/shulkerscript-vscode"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "shulkerscript",
      "aliases": ["Shulkerscript", "shulkerscript"],
      "extensions": [".shu"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "shulkerscript",
      "scopeName": "source.shulkerscript",
      "path": "./syntaxes/shulkerscript.tmLanguage.json"
    }],
    "snippets": [{
        "language": "shulkerscript",
        "path": "./snippets.json"
    }]
  }
}
